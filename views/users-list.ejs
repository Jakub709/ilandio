<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--========== BOX ICONS ==========-->
    <link rel="icon" href="images/favicon.png" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css"
    />

    <!--========== CSS ==========-->
    <link
      rel="stylesheet"
      href="/css/general/generalInner.css
  "
    />
    <link
      rel="stylesheet"
      href="/css/usersList/usersList.css
    "
    />

    <!--========== JQUERY ==========-->
    <script
      src="https://code.jquery.com/jquery-3.5.1.min.js"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>

    <title>Seznam uživatelů</title>
  </head>
  <body>
    <!--========== HEADER ==========-->
    <header class="header">
      <div class="header__container">
        <img src="images/ilandio_logo.png" alt="" class="header__img" />

        <p class="header__logo">Uživatelé</p>

        <div class="header__search">
          <input type="search" placeholder="Hledat" class="header__input" />
          <i class="bx bx-search header__icon"></i>
        </div>

        <div class="header__toggle">
          <i class="bx bx-menu" id="header-toggle"></i>
        </div>
      </div>
    </header>

    <!--========== NAV ==========-->
    <div class="nav" id="navbar">
      <nav class="nav__container">
        <div>
          <a
            href="https://www.facebook.com/ilandio/"
            target="_blank"
            class="nav__link nav__logo"
          >
            <i class="bx bxs-disc nav__icon"></i>
            <span class="nav__logo-name">Novinky</span>
          </a>

          <div class="nav__list">
            <div class="nav__items">
              <h3 class="nav__subtitle">Příspěvky</h3>

              <a href="/posts-list" class="nav__link">
                <i class="bx bx-task nav__icon"></i>
                <span class="nav__name">Seznam příspěvků</span>
              </a>

              <a href="/my-posts" class="nav__link">
                <i class="bx bx-pencil nav__icon"></i>
                <span class="nav__name">Moje příspěvky</span>
              </a>
            </div>

            <div class="nav__items">
              <h3 class="nav__subtitle">Uživatelé</h3>

              <a href="/users-list" class="nav__link">
                <i class="bx bx-user nav__icon"></i>
                <span class="nav__name">Seznam uživatelů</span>
              </a>
              <div class="nav__dropdown">
                <a href="#" class="nav__link">
                  <i class="bx bx-cog nav__icon"></i>
                  <span class="nav__name">Můj profil</span>
                  <i
                    class="bx bx-chevron-down nav__icon nav__dropdown-icon"
                  ></i>
                </a>

                <div class="nav__dropdown-collapse">
                  <div class="nav__dropdown-content">
                    <a href="/my-profile" class="nav__dropdown-item"
                      >Editovat profil</a
                    >
                    <a href="/my-profile-watch" class="nav__dropdown-item"
                      >Zobrazit profil</a
                    >
                  </div>
                </div>
              </div>
            </div>

            <div class="nav__items">
              <h3 class="nav__subtitle">Ostatní</h3>
              <a href="/bank" class="nav__link">
                <i class="bx bx-coin-stack nav__icon"></i>
                <span class="nav__name">Banka</span>
              </a>
              <a href="/messages" class="nav__link">
                <i class="bx bx-message-rounded nav__icon"></i>
                <span class="nav__name">Zprávy</span>
                <span id="messagesBadge"></span>
              </a>
            </div>
          </div>
        </div>

        <a href="/logout" class="nav__link nav__logout">
          <i class="bx bx-log-out nav__icon"></i>
          <span class="nav__name">Odhlásit se</span>
        </a>
      </nav>
    </div>

    <!--========== CONTENTS ==========-->
    <main>
      <div class="go-up-here"></div>
      <div class="my-post-page">
        <div class="container">
          <% users.forEach(function(user) { %>
          <div id="my-posts" class="my-post">
            <div class="grid grid--2-cols-post">
              <div class="my-post-mainImage">
                <img src="<%= user.profilePic %>" alt="user" />
              </div>
              <div class="my-post-body">
                <h4><%= user.name %></h4>
                <div class="social-buttons">
                  <a
                    href="http://www.facebook.com/<%= user.facebook %>"
                    class="social-buttons__button social-button social-button--facebook"
                    aria-label="Facebook"
                    target="_blank"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      viewBox="0 0 512 512"
                    >
                      <!--! Font Awesome Pro 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. -->
                      <path
                        d="M504 256C504 119 393 8 256 8S8 119 8 256c0 123.78 90.69 226.38 209.25 245V327.69h-63V256h63v-54.64c0-62.15 37-96.48 93.67-96.48 27.14 0 55.52 4.84 55.52 4.84v61h-31.28c-30.8 0-40.41 19.12-40.41 38.73V256h68.78l-11 71.69h-57.78V501C413.31 482.38 504 379.78 504 256z"
                      />
                    </svg>
                  </a>
                  <a
                    href="http://www.linkedin.com/<%= user.linkedin %>"
                    class="social-buttons__button social-button social-button--linkedin"
                    aria-label="LinkedIn"
                    target="_blank"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      viewBox="0 0 448 512"
                    >
                      <!--! Font Awesome Pro 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. -->
                      <path
                        d="M416 32H31.9C14.3 32 0 46.5 0 64.3v383.4C0 465.5 14.3 480 31.9 480H416c17.6 0 32-14.5 32-32.3V64.3c0-17.8-14.4-32.3-32-32.3zM135.4 416H69V202.2h66.5V416zm-33.2-243c-21.3 0-38.5-17.3-38.5-38.5S80.9 96 102.2 96c21.2 0 38.5 17.3 38.5 38.5 0 21.3-17.2 38.5-38.5 38.5zm282.1 243h-66.4V312c0-24.8-.5-56.7-34.5-56.7-34.6 0-39.9 27-39.9 54.9V416h-66.4V202.2h63.7v29.2h.9c8.9-16.8 30.6-34.5 62.9-34.5 67.2 0 79.7 44.3 79.7 101.9V416z"
                      />
                    </svg>
                  </a>
                  <a
                    href="https://github.com/<%= user.github %>"
                    class="social-buttons__button social-button social-button--github"
                    aria-label="GitHub"
                    target="_blank"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      viewBox="0 0 496 512"
                    >
                      <!--! Font Awesome Pro 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. -->
                      <path
                        d="M165.9 397.4c0 2-2.3 3.6-5.2 3.6-3.3.3-5.6-1.3-5.6-3.6 0-2 2.3-3.6 5.2-3.6 3-.3 5.6 1.3 5.6 3.6zm-31.1-4.5c-.7 2 1.3 4.3 4.3 4.9 2.6 1 5.6 0 6.2-2s-1.3-4.3-4.3-5.2c-2.6-.7-5.5.3-6.2 2.3zm44.2-1.7c-2.9.7-4.9 2.6-4.6 4.9.3 2 2.9 3.3 5.9 2.6 2.9-.7 4.9-2.6 4.6-4.6-.3-1.9-3-3.2-5.9-2.9zM244.8 8C106.1 8 0 113.3 0 252c0 110.9 69.8 205.8 169.5 239.2 12.8 2.3 17.3-5.6 17.3-12.1 0-6.2-.3-40.4-.3-61.4 0 0-70 15-84.7-29.8 0 0-11.4-29.1-27.8-36.6 0 0-22.9-15.7 1.6-15.4 0 0 24.9 2 38.6 25.8 21.9 38.6 58.6 27.5 72.9 20.9 2.3-16 8.8-27.1 16-33.7-55.9-6.2-112.3-14.3-112.3-110.5 0-27.5 7.6-41.3 23.6-58.9-2.6-6.5-11.1-33.3 2.6-67.9 20.9-6.5 69 27 69 27 20-5.6 41.5-8.5 62.8-8.5s42.8 2.9 62.8 8.5c0 0 48.1-33.6 69-27 13.7 34.7 5.2 61.4 2.6 67.9 16 17.7 25.8 31.5 25.8 58.9 0 96.5-58.9 104.2-114.8 110.5 9.2 7.9 17 22.9 17 46.4 0 33.7-.3 75.4-.3 83.6 0 6.5 4.6 14.4 17.3 12.1C428.2 457.8 496 362.9 496 252 496 113.3 383.5 8 244.8 8zM97.2 352.9c-1.3 1-1 3.3.7 5.2 1.6 1.6 3.9 2.3 5.2 1 1.3-1 1-3.3-.7-5.2-1.6-1.6-3.9-2.3-5.2-1zm-10.8-8.1c-.7 1.3.3 2.9 2.3 3.9 1.6 1 3.6.7 4.3-.7.7-1.3-.3-2.9-2.3-3.9-2-.6-3.6-.3-4.3.7zm32.4 35.6c-1.6 1.3-1 4.3 1.3 6.2 2.3 2.3 5.2 2.6 6.5 1 1.3-1.3.7-4.3-1.3-6.2-2.2-2.3-5.2-2.6-6.5-1zm-11.4-14.7c-1.6 1-1.6 3.6 0 5.9 1.6 2.3 4.3 3.3 5.6 2.3 1.6-1.3 1.6-3.9 0-6.2-1.4-2.3-4-3.3-5.6-2z"
                      />
                    </svg>
                  </a>
                  <p>|</p>
                  <a
                    href="/user-profile/<%= user._id %>"
                    class="social-buttons__button social-button social-button--watch social-buttons-posts"
                    aria-label="GitHub"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      viewBox="0 0 576 512"
                    >
                      <!--! Font Awesome Pro 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. -->
                      <path
                        d="M279.6 160.4C282.4 160.1 285.2 160 288 160C341 160 384 202.1 384 256C384 309 341 352 288 352C234.1 352 192 309 192 256C192 253.2 192.1 250.4 192.4 247.6C201.7 252.1 212.5 256 224 256C259.3 256 288 227.3 288 192C288 180.5 284.1 169.7 279.6 160.4zM480.6 112.6C527.4 156 558.7 207.1 573.5 243.7C576.8 251.6 576.8 260.4 573.5 268.3C558.7 304 527.4 355.1 480.6 399.4C433.5 443.2 368.8 480 288 480C207.2 480 142.5 443.2 95.42 399.4C48.62 355.1 17.34 304 2.461 268.3C-.8205 260.4-.8205 251.6 2.461 243.7C17.34 207.1 48.62 156 95.42 112.6C142.5 68.84 207.2 32 288 32C368.8 32 433.5 68.84 480.6 112.6V112.6zM288 112C208.5 112 144 176.5 144 256C144 335.5 208.5 400 288 400C367.5 400 432 335.5 432 256C432 176.5 367.5 112 288 112z"
                      />
                    </svg>
                  </a>
                </div>
                <p class="my-post-content"><%= user.about %></p>
              </div>
            </div>

            <div class="my-post-about">
              <div class="my-post-tags grid grid--5-cols">
                <span class="tag tag-two"><%= user.job %></span>
                <span class="tag tag-two"><%= user.position %></span>
                <span class="tag tag-two"><%= user.region %></span>
                <span class="tag tag-one"
                  ><b><%= user.followersCounter %></b> sledujících</span
                >
                <span class="tag tag-one"
                  ><b><%= user.followingCounter %></b> sleduje</span
                >
              </div>
            </div>
          </div>
          <% }); %>
        </div>
        <div class="no-results">
          <% if(usersCounter<=0){ %>
          <img
            src="data:image/png;base64,"
            alt=""
          />

          <p>Žádné uživatele jsme nenašli :-(</p>
          <% } else{ %>
          <p></p>
          <% } %>
        </div>
        <div class="next-previous">
          <!-- url =  /posts-list?&type=nabídka-->
          <a href="<%= url %>&page=<%= currentPage-1 %>"
            ><%= textPrevious %></a
          >
          <a href="<%= url %>&page=<%= currentPage+1 %>"
            ><%= textNext %></a
          >
        </div>
      </div>
    </main>
    <button id="go-top">
      <?xml version="1.0" ?><svg
        id="go-top-icon"
        viewBox="0 0 96 96"
        xmlns="http://www.w3.org/2000/svg"
      >
        <path
          d="M82.6074,62.1072,52.6057,26.1052a6.2028,6.2028,0,0,0-9.2114,0L13.3926,62.1072a5.999,5.999,0,1,0,9.2114,7.6879L48,39.3246,73.396,69.7951a5.999,5.999,0,1,0,9.2114-7.6879Z"
        />
      </svg>
    </button>
    <script src="/js/usersList.js"></script>
    <script src="/js/commonLoggedIn.js"></script>
  </body>
</html>
